<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="callback"
            type="com.example.vyapaar.dashboard.contract.BaseContract" />

        <variable
            name="viewModel"
            type="com.example.vyapaar.dashboard.ui.dashboard.DashboardViewModel" />
    </data>

    <androidx.recyclerview.widget.RecyclerView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/list"
        android:name="com.example.vyapaar.dashboard.ui.dashboard.DashboardItemFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        app:layoutManager="LinearLayoutManager"
        tools:context=".dashboard.ui.dashboard.DashboardItemFragment"
        tools:listitem="@layout/fragment_dashboard_item" />
</layout>